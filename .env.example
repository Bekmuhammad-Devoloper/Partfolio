# Copy this file to .env on the server and fill values before starting
# Do NOT commit real secrets to the repo

# Server
PORT=3000
NODE_ENV=production

# Database (Postgres example)
DATABASE_URL=postgres://user:password@localhost:5432/dbname

# JWT
JWT_SECRET=your_jwt_secret_here

# Payment / 3rd party placeholders
CLICK_MERCHANT_ID=
CLICK_SECRET=

# Other
SOME_API_KEY=
